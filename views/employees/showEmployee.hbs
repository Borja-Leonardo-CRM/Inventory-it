<div class="container">
  <table class="table table-hover">
    <thead>
      <tr>
        <th>ID</th>
        <th>NAME</th>
        <th>DEPARTMENT</th>
        <th>EQUIPMENT</th>
      </tr>
    </thead>
    <tbody>
      {{#each employees}}
      <tr>
        <td>{{_id}}</td>
        <td>{{ name}}</td>
        <td>{{ department}}</td>
        <td>{{ equipmentsId}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>